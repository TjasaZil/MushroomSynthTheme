  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#87CEFA"
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "scope": ["variable", "string variable"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "scope": "storage.modifier.package",
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#FF1493"
      }
    }
  ]
}
